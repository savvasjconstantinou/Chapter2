#bring data in
Bg <- read.csv("C:/Users/Savvas Constantinou/Desktop/WTCRISPRBg.csv", header = TRUE, colClasses = c("numeric", "factor", "factor", "factor"))
head(Bg)


#group amp by treatment
ggboxplot(Bg, x = "Length", y = "Amplitude", add = "point", color = "Group")

#determine number of observations in each group
label_df <- Bg %>% count(Group, Length)
label_df


#are there differences?
aov <- aov(Amplitude ~ Length, data= Bg)
summary(aov)

posthoc <- TukeyHSD(aov)
posthoc
